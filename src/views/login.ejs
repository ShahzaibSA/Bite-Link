<body class='html-body'>
     <nav class="navbar navbar-expand-lg bg-secondary fixed-top" id="mainNav">
        <div>
            <img src="/assets/img/logo.svg" alt="logo">
            <span>BITE LINK</span>
        </div>
    </nav>
    <div class='body-container'>
        <!-- <div class='container message-div' style='width: 900px;'></div> -->
        <!-- Sing in  Form -->
        <section class="sign-in">
            <div class='message-div'></div>
            <div class="container">
                <div class="signin-content">
                    <div class="signin-image">
                        <figure><img src="/assets/img/signin-image.jpg" alt="sing up image"></figure>
                        <a href="/signup" class="signup-image-link">Create an account</a>
                    </div>

                    <div class="signin-form" style='margin-top: 5vh;'>
                        <h2 class="form-title">Sign in</h2>
                        <form class="login-form" id="login-form">
                            <div class="form-group">
                                <label for="email"><i class="zmdi zmdi-email"></i></label>
                                <input value='shahzaibsaa1@gmail.com' type="email" name="email" id="email" placeholder="Your Email"/>
                            </div>
                            <div class="form-group">
                                <label for="your_pass"><i class="zmdi zmdi-lock"></i></label>
                                <input value='shah123' type="password" name="password" id="your_pass" placeholder="Password"/>
                            </div>
                            <div class="form-group">
                                <input type="checkbox" name="remember-me" id="remember-me" class="agree-term" />
                                <label for="remember-me" class="label-agree-term"><span><span></span></span>Remember me</label>
                            </div>
                            <div class='d-flex d-inline'>
                                <div class="form-group form-button">
                                    <input type="submit" name="signin" id="signin" class="form-submit" value="Log in"/>
                                </div>
                                <div class="spinner-div d-none my-4 mx-4" role="status">
                                    <span class="sr-only">Loading...</span>
                                </div>
                            </div>

                        </form>
                        <!-- <div class="social-login">
                            <span class="social-label">Or login with</span>
                            <ul class="socials">
                                <li><a href="#"><i class="display-flex-center zmdi zmdi-facebook"></i></a></li>
                                <li><a href="#"><i class="display-flex-center zmdi zmdi-twitter"></i></a></li>
                                <li><a href="#"><i class="display-flex-center zmdi zmdi-google"></i></a></li>
                            </ul>
                        </div> -->
                    </div>
                </div>
            </div>
        </section>

    </div>

